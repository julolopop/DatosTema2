<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.DatosTema2">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.DatosTema2.MenuActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.DatosTema2.AlarmaActivity" />
        <activity android:name="com.example.DatosTema2.CalendarioActivity" />
        <activity android:name="com.example.DatosTema2.NavegadorActivity" />
        <activity android:name="com.example.DatosTema2.ImagenesActivity" />
        <activity android:name="com.example.DatosTema2.ConversorActivity" />
        <activity android:name="com.example.DatosTema2.SubidaFicheroActivity" />
        <activity android:name="com.example.DatosTema2.AgendaActivity" />
        <activity android:name="com.example.DatosTema2.ContadorActivity"></activity>
    </application>

</manifest>